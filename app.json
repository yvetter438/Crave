{
  "expo": {
    "name": "Crave",
    "slug": "FoodyDemo",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crave",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/login-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#FEFEFE"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yvetter.FoodyDemo",
      "buildNumber": "7",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photos for uploading images.",
        "NSMicrophoneUsageDescription": "This app needs access to the microphone for videos.",
        "UIRequiredDeviceCapabilities": [
          "telephony",
          "telephony"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FEFEFE"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.crave.social"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app access your photos to let you share them with your friends."
        }
      ],
      "expo-video",
      "expo-web-browser",
      "expo-localization",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "crave-ek"
        }
      ],
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd9fe797-e750-4e1c-96fc-62b043ca8c08"
      }
    }
  }
}
